<%- include('partials/header') -%>

<div class="row spaced">
<% for(let i=0; i<cards.length; i++){ %>
    <div class="col-md-4 col-sm-6 card-space-bottom">
        <div class="card back-color">
            <img class="card-img-top" src="<%= cards[i].src %>" alt="">
            <div class="card-body ">
                <h5 class="card-title"><%= cards[i].cardTitle %></h5>
                <p class="card-text"><%= cards[i].cardText %></p>
                <a href="<%= cards[i].link %>" class="btn btn-primary">Click Here to Proceed</a>
            </div>
        </div>
    </div>
<% }; %>
</div>

<%- include('partials/footer') -%>